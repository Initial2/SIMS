        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/stuAge"
        android:layout_width="50dp"
        android:layout_height="25dp"
